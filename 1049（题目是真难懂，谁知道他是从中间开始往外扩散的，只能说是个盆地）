# zoj
因为杭电oj打不开了，估计是要用vpn了，所以被迫做zoj了
#include <stdio.h>
#include <math.h>
int main()
{
	int n;
	scanf("%d", &n);
	int i;
	double x, y;
	const double pi = 3.141592657;
	for (i = 1; i <= n; i++)
	{
		scanf("%lf %lf", &x, &y);
		double r;
		r = sqrt(x * x + y * y);
		double s;
		s = pi * r * r/2.0;
		int year;
		year = ceil(s / 50.0);
		printf("Property %d: This property will begin eroding in year %d.\n", i, year);

	}
	printf("END OF OUTPUT.\n");
	return 0;
}
